h3 级联列表管理
  small Cascade List Management

.panel.panel-default
    .panel-heading
        | 收入类型级联
    .table-responsive(style='overflow-x: visible;')
        table.table.table-striped.table-bordered.table-hover
          thead
            tr
              th 名称
              th.text-center 一级级联
              th.text-center 二级级联
          tbody
           tr
              td 通用类型
              td.form-inline.editable-wrap.text-center
                .btn-group(dropdown='dropdown')
                    button.btn.btn-default.btn-sm(type='button', ng-disabled='disabled', dropdown-toggle="")
                      em.fa.fa-angle-down.fa-fw
                      | {{cascade.incomeCommonOneSelected?cascade.incomeCommonOneSelected.name:'请选择要操作的级联'}}
                    ul.dropdown-menu(role='menu')
                      li(ng-if='cascade.incomeCommonBrige.length<1')
                        a 暂无级联选项
                      li(ng-repeat='(key,value) in cascade.incomeCommonBrige track by $index')
                        a(ng-bind='key' ng-click='cascade.changeOneSelected(cascade.incomeCommon,cascade.incomeCommonOneSelected,cascade.incomeCommonTwoSelected,key,value)')
                .editable-controls.form-group
                  input.editable-has-buttons.editable-input.form-control.input-sm(type='text' ng-model='cascade.incomeCommonOneSelected.name')
                  span.editable-buttons
                    button.btn.btn-success.btn-sm(type='button' ng-click='cascade.layoutOneEdit(cascade.incomeCommonOneSelected,cascade.incomeCommonBrige)')
                      span.fa.fa-check
                    button.btn.btn-danger.btn-sm(type='button' ng-click='cascade.layoutOneDelect(cascade.incomeCommonOneSelected,cascade.incomeCommonBrige)')
                      span.fa.fa-minus
                  span.editable-buttons
                    button.btn.btn-default.btn-sm(type='button' ng-click='cascade.layoutOneAdd(cascade.incomeCommonOneSelected,cascade.incomeCommonBrige,0)')
                      span.fa.fa-plus
              td.form-inline.editable-wrap.text-center
                .btn-group(dropdown='dropdown')
                  button.btn.btn-default.btn-sm(type='button', ng-disabled='disabled', dropdown-toggle="")
                    em.fa.fa-angle-down.fa-fw
                    | {{cascade.incomeCommonTwoSelected.name?cascade.incomeCommonTwoSelected.name:'请选择要操作的级联'}}
                  ul.dropdown-menu(role='menu')
                    li(ng-if='!cascade.incomeCommon[cascade.incomeCommonOneSelected.name]||cascade.incomeCommon[cascade.incomeCommonOneSelected.name].length<1')
                      a 暂无级联选项
                    li(ng-repeat='item in cascade.incomeCommon[cascade.incomeCommonOneSelected.name] track by $index' ng-if='cascade.incomeCommonOneSelected')
                      a(value='item.layout_two_id' ng-bind='item.name' ng-click='cascade.changeSelected(cascade.incomeCommonTwoSelected,item.name)')
                .editable-controls.form-group
                    input.editable-has-buttons.editable-input.form-control.input-sm(type='text' ng-model='cascade.incomeCommonTwoSelected.name')
                    span.editable-buttons
                      button.btn.btn-success.btn-sm(type='button' ng-click='cascade.layoutTwoEdit(cascade.incomeCommonTwoSelected,cascade.incomeCommon[cascade.incomeCommonOneSelected.name])')
                        span.fa.fa-check
                      button.btn.btn-danger.btn-sm(type='button' ng-click='cascade.layoutTwoDelect(cascade.incomeCommonTwoSelected,cascade.incomeCommon[cascade.incomeCommonOneSelected.name])')
                        span.fa.fa-minus
                  span.editable-buttons
                    button.btn.btn-default.btn-sm(type='button' ng-click='cascade.layoutTwoAdd(cascade.incomeCommonOneSelected,cascade.incomeCommonTwoSelected,cascade.incomeCommon[cascade.incomeCommonOneSelected.name],0)')
                      span.fa.fa-plus

                  span.editable-buttons
                    button.btn.btn-success.btn-sm(type='button' ng-click='cascade.editPlace(cascade.placeSelected)')
                      span.fa.fa-check
                    button.btn.btn-danger.btn-sm(type='button' ng-click='cascade.deletePlace(cascade.placeSelected)')
                      span.fa.fa-minus
                  span.editable-buttons
                    button.btn.btn-default.btn-sm(type='button' ng-click='cascade.addPlace(cascade.placeSelected)')
                      span.fa.fa-plus



